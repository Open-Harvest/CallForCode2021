    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "mocha": "^8.0.1",
    "prettier": "^2.0.4",
    "puppeteer": "^8.0.0",
    "rollup": "^2.4.0",
    "semver": "^7.3.2",
    "terser": "^5.2.1",
    "typescript": "^4.0.2"
  }
