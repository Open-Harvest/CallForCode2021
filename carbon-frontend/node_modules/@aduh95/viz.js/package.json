{
  "name": "@aduh95/viz.js",
  "version": "3.4.0",
  "description": "A hack to put Graphviz on the web.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "browser": "./dist/index.mjs",
