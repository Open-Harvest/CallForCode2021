  "files": [
    "dist/index.cjs",
    "dist/index.mjs",
    "dist/index.d.ts",
    "dist/types.d.ts",
    "dist/render.node.mjs",
    "dist/render.browser.js",
    "dist/render.wasm",
    "dist/render_async.cjs",
    "dist/render_async.d.ts",
    "dist/render_sync.cjs",
    "dist/render_sync.d.ts",
    "async/index.js",
    "async/index.d.ts",
    "sync/index.js",
    "sync/index.d.ts",
    "wasm",
    "worker"
  ],
  "contributors": [
